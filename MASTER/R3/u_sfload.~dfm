  object q_nofact: TOracleDataSet
    SQL.Strings = (
      'SELECT COUNT(*) AS NOFACT'
      'FROM KVIT K,MONTH M,KLS_DOG D'
      'WHERE K.DATE_KVIT=:DATEKVIT'
      'AND K.FLG_OPERDATA=0'
      'AND K.BILL_ID=0'
