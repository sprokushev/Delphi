  object q_Prod: TOracleDataSet
    SQL.Strings = (
      'SELECT DISTINCT'
      '  KLS_PROD.ID_NPR,'
      '  KLS_PROD.NAME_NPR as NAME_NPR'
      'FROM KLS_PROD,PLAN_POST'
      'WHERE PLAN_POST.PROD_ID_NPR=KLS_PROD.ID_NPR'
      '  AND PLAN_POST.PLAN_PER_ID=0'
