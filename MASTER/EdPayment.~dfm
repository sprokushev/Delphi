  object q_Dog: TOracleDataSet
    SQL.Strings = (
      'SELECT'
      '  kls_dog.id,'
      '  kls_dog.dog_number,'
      '  kls_predpr.predpr_name,'
      '  kls_predpr.banks_id, kls_predpr.rs'
      'FROM kls_dog,kls_predpr'
      
        'WHERE kls_dog.predpr_id=kls_predpr.id and kls_dog.dog_year>=2004' +
        ' and agent_id=8'
      'ORDER BY dog_number')
