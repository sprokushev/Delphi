  object oraPR17: TOracleDataSet
    SQL.Strings = (
      'select /*+ ALL_ROWS*/'
      
        ' m.nom_zd, m.TONN_DECLARED,m.date_plan,m.input_date,m.planstru_i' +
        'd,pn.taxgr_code_new, pn.taxgr_code,'
      
        ' pn.MODIF,pn.MOD_MODIF,s.STAN_NAME,trim(p.GD_KOD)||'#39'/'#39'||trim(p.O' +
        'KPO) as agnfifo,pn.taxgr_code,pn.taxgr_code_new'
      'from month m, kls_planstru ps,'
      'kls_vid_otgr v,'
      'kls_prod_nomenklator pn,'
      'kls_stan s, kls_predpr p,kls_dog d,ZAKAZ Z,zakaz unp'
      'where m.PROD_ID_NPR=pn.PROD and z.planstru_id=ps.id'
      
        'and m.load_abbr=v.load_abbr and (unp.id=m.zakaz_Id and unp.IS_AG' +
        'ENT=1)'
      'and m.STAN_ID=s.ID and unp.LINK_ID=z.ID'
      'and m.POLUCH_ID=p.ID and z.dog_id=d.id (+)'
      'and z.IS_AGENT=2 and d.predpr_id=2641'
      'and m.nazn_otg_id<>2 and m.TONN_DECLARED>0'
      
        'and (v.load_type_id=1 or v.load_type_id=6 or v.load_type_id=5) a' +
        'nd pn.is_actual<>0'
      
        'and not exists (select month_id from parus_fin where m.NOM_ZD = ' +
        'parus_fin.month_id and task='#39'DepartmentsOrders'#39')'
      ''
      ''
      ''
      ''
      ''
      ''
      ''
      ''
      ''
      ''
      'order by m.NOM_ZD'
      '')
    QBEDefinition.QBEFieldDefs = {
